<tr id="tr<%= $lkey %>" class="<%= $class %>" onmouseover="tr_select_over(this);" onmouseout="tr_select_out(this, '<%= $class %>');">
  <td class=edit valign=top width="150px" colspan=3><%= $lkey->{name} =%>:</td>
  <td class=read valign=top>
  	<% if($value ne '0000-00-00'){ %><%= $value %><% } else { %>Не выбрано<% } %>
  </td>
</tr>